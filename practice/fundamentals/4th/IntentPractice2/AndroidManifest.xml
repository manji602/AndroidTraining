<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.practice.intent.med"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jp.mixi.practice.intent.med.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name="jp.mixi.practice.intent.med.android.intent.action.FIRST">
            <intent-filter>
                <action android:name="jp.mixi.practice.intent.med.android.intent.action.FIRST"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name="jp.mixi.practice.intent.med.android.intent.action.SECOND">
            <intent-filter>
                <action android:name="jp.mixi.practice.intent.med.android.intent.action.SECOND"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name="jp.mixi.practice.intent.med.android.intent.action.THIRD">
            <intent-filter>
                <action android:name="jp.mixi.practice.intent.med.android.intent.action.THIRD"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>